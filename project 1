Age_of_User = int(input(" Enter the number that represents your age ---->   "))
def Classify_Age(x):
    if x > 0 and x <= 12:
        return("You are a child.")
    elif x > 12 and x <= 18:
        return("You are a teenager.")
    elif x > 18 and x <= 64:
        return("You are an adult.")
    elif x > 64:
        return (" You are a senior. ")
    else:
        return("You are not human.")
print(Classify_Age(Age_of_User))
Time_Of_Day = int(input(" Enter the number that represents the current time of day based on a 24 hour format :  "))
def Day_Time(x):
    if x >= 5 and x <= 11:
        return(" Morning ")
    elif x >= 12 and x <= 16:
        return(" Afternoon ")
    elif x >= 17 and x <= 20:
        return(" Evening ")
    elif (21 <= x <= 24) or (0 < x <= 4):
        return ("Night")
    else:
        return(" You do not live in the Milky way.")
print(Day_Time(Time_Of_Day))=
Inputed_Number = int(input(" Enter a number, specifically an integer :  "))
def Divisibility_Checker(x):
    if x % 5 == 0:
        return("Your number is divisible by 5.")
    else:
        return("Your number is not divisible by 5.")
print(Divisibility_Checker(Inputed_Number))